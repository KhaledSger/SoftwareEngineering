<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.DoctorController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" layoutX="5.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../healthcare.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backBtn" ellipsisString="" layoutX="13.0" layoutY="27.0" mnemonicParsing="false" onAction="#backBtnAction" prefHeight="26.0" prefWidth="39.0" text="back" underline="true">
         <font>
            <Font size="10.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <ImageView fitHeight="33.0" fitWidth="39.0" layoutX="61.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../People-Doctor-Male-icon.png" />
         </image>
      </ImageView>
      <Text fx:id="welcome_text" layoutX="94.0" layoutY="45.0" strokeType="OUTSIDE" strokeWidth="0.0" text="text" wrappingWidth="118.93670654296875" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="22.0" layoutX="336.0" layoutY="391.0" stroke="WHITE" strokeType="INSIDE" width="138.0" />
      <Button fx:id="view_patients" layoutX="24.0" layoutY="123.0" mnemonicParsing="false" onAction="#view_patients_handler" prefHeight="72.0" prefWidth="130.0" text="View Your Patients" />
      <Button fx:id="view_apps" layoutX="505.0" layoutY="123.0" mnemonicParsing="false" onAction="#view_apps_handler" prefHeight="72.0" prefWidth="130.0" text="View Your Appointments" wrapText="true" />
      <ListView fx:id="patients_list" layoutX="24.0" layoutY="224.0" prefHeight="350.0" prefWidth="200.0" />
      <ListView fx:id="apps_list" layoutX="505.0" layoutY="227.0" prefHeight="350.0" prefWidth="200.0" />
   </children>
</AnchorPane>
