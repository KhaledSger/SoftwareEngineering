<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="407.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.PatientController">
    <children>
        <VBox layoutX="-1.0" prefHeight="407.0" prefWidth="320.0">
            <children>
                <TextField text="Reserve An Appointment :" />
                <MenuButton fx:id="SpecializedDoctorMenuBtn" mnemonicParsing="false" onAction="#SpecializedDoctorAction" prefHeight="25.0" prefWidth="247.0" text="Specialized Doctor Appointment">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Action 1" />
                        <MenuItem mnemonicParsing="false" text="Action 2" />
                    </items>
                </MenuButton>
                <Button fx:id="FamilyDocButton" mnemonicParsing="false" onAction="#FamilyDocAction" text="Family Doctor Or Pediatrician Appointment " />
                <Button fx:id="NurseAppBtn" mnemonicParsing="false" onAction="#NurseAppAction" prefHeight="25.0" prefWidth="247.0" text="Nurse Appointment" />
                <Button fx:id="LabTest" mnemonicParsing="false" onAction="#LabTestAction" prefHeight="25.0" prefWidth="248.0" text="Lab Test Appointment" />
                <Button fx:id="CovidVaccineBtn" mnemonicParsing="false" onAction="#CovidVaccineAction" prefHeight="13.0" prefWidth="249.0" text="Covid 19 Vaccination Appointment" />
                <Button fx:id="CovidTestBtn" mnemonicParsing="false" onAction="#CovidTestAction" prefHeight="22.0" prefWidth="249.0" text="Covid 19 Test Appointment" />
                <TextField text="Generate Green Pass :" />
                <Button fx:id="GreenPassBtn" mnemonicParsing="false" onAction="#GreenPassAction" prefHeight="25.0" prefWidth="253.0" text="Green Pass" />
            </children>
        </VBox>
    </children>
</AnchorPane>
